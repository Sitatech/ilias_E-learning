<?xml version="1.0" encoding="UTF-8"?>
<ilLayout>
	<ilFrame name="content" cols="50%,50%">
		<ilFrame name="maincontent">
			<StructureObject template="tpl.page.html" template_location="module">
				<ilPage/>
				<ilLMNavigation/>
				<ilLocator/>
				<ilMainMenu/>
				<ilLMMenu />
				<ilLMHead />
				<ilLMSubMenu />
				<ilLMNotes />
			</StructureObject>
			<PageObject template="tpl.page.html" template_location="module">
				<ilPage/>
				<ilLMNavigation/>
				<ilLocator/>
				<ilMainMenu/>
				<ilLMMenu />
				<ilLMHead />
				<ilLMSubMenu />
				<ilLMNotes />
			</PageObject>
		</ilFrame>
		<ilFrame rows="66%,34%">
			<ilFrame name="topright" template="tpl.blank.html" template_location="module">
				<PageObject template="tpl.page.html" template_location="module">
					<ilPage/>
				</PageObject>
				<MediaObject template="tpl.fullscreen.html" template_location="module">
					<ilMedia/>
				</MediaObject>
			</ilFrame>
			<ilFrame name="botright" template="tpl.blank.html" template_location="module">
				<GlossaryItem template="tpl.glossary_term_output.html" template_location="module">
					<ilGlossary/>
				</GlossaryItem>
			</ilFrame>
		</ilFrame>
	</ilFrame>
	<ilLinkTargets>
		<LinkTarget Type="New" Frame="_blank"/>
		<LinkTarget Type="FAQ" Frame="topright"/>
		<LinkTarget Type="Glossary" Frame="botright"/>
		<LinkTarget Type="Media" Frame="topright"/>
	</ilLinkTargets>
</ilLayout>
